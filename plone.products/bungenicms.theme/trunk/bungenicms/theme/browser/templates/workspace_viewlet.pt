<tal:get define="member view/member">
    <li tal:condition="member/getId">
        <a href="#" 
            title="draft"
            class="workspace"
            tal:attributes="href string:${view/portal_url}/workspace/my-documents/draft">
        </a>
    </li>
    <li tal:condition="member/getId">
        <a href="#" 
            title="Inbox"
            class="inbox"
