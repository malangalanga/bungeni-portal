<workspace>

    <state id="draft">
        <tab id="draft" roles="Drafter" />
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate" />
        <tab id="pending" roles="Owner" />
    </state>
    <state id="inadmissible">
        <tab id="archive" roles="ClerkSenate Owner" />
    </state>
    <state id="admissible">
        <tab id="inbox" roles="ClerkSenate"/>
        <tab id="pending" roles="SpeakerSenate Owner" />
    </state>
    <state id="endorsed">
        <tab id="inbox" roles="ClerkSenate" />
        <tab id="pending" roles="Owner SpeakerSenate" />
    </state>
    <state id="not_endorsed">
        <tab id="archive" roles="ClerkSenate Owner SpeakerSenate" />
    </state>
    <state id="presented">
        <tab id="archive" 
	     roles="Owner ClerkSenate SpeakerSenate" />
    </state>
    <state id="withdrawn">
        <tab id="archive"
	     roles="Owner ClerkSenate SpeakerSenate" />
    </state>
</workspace>
