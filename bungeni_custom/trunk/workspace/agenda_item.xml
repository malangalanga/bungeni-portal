<workspace id="agendaitem">
    <state id="working_draft">
        <tab id="draft" roles="Owner"/>
    </state>
    <state id="draft">
        <tab id="draft" roles="Owner"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="Owner"/>
        <tab id="pending" roles="ClerkSenate ClerkAssembly"/>
    </state>
    <state id="reviewed">
        <tab id="inbox" roles="SpeakerSenate SpeakerAssembly"/>
        <tab id="pending" roles="ClerkSenate ClerkAssembly"/>
    </state>
     <state id="inadmissible">
        <tab id="archive" 
	     roles="SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly
		    Owner"/>
    </state>
    <state id="admissible">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly"/>
        <tab id="pending" roles="Owner SpeakerSenate SpeakerAssembly"/>
    </state>
    <state id="schedule_pending">
        <tab id="schedule" 
	     roles="Owner
		    Signatory
		    SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly"/>
    </state>
    <state id="scheduled">
        <tab id="schedule" 
	     roles="Owner
		    Signatory
		    SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly"/>
    </state>
    <state id="adjourned">
        <tab id="schedule" 
	     roles="ClerkSenate ClerkAssembly
		    SpeakerSenate SpeakerAssembly
		    Owner"/>
    </state>
    <state id="dropped">
        <tab id="archive" 
	     roles="SpeakerSenate SpeakerAssembly
		    Owner"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" 
	     roles="Owner
		    ClerkSenate ClerkAssembly
		    SpeakerSenate SpeakerAssembly"/>
    </state>
</workspace>
