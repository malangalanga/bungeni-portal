<workspace id="agendaitem">
    <state id="draft">
        <tab id="draft" roles="Drafter"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="Owner"/>
        <tab id="pending" roles="ClerkSenate ClerkAssembly"/>
    </state>
     <state id="inadmissible">
        <tab id="archive" 
	     roles="SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly
		    Owner"/>
    </state>
    <state id="admissible">
        <tab id="schedule" 
	     roles="SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly"/>
        <tab id="pending" 
	     roles="Owner Signatory"/>
    </state>

    <state id="scheduled">
        <tab id="schedule" 
	     roles="SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly"/>
        <tab id="pending" 
	     roles="Owner Signatory"/>
    </state>
    <state id="adjourned">
        <tab id="schedule" 
	     roles="SpeakerSenate SpeakerAssembly
		    ClerkSenate ClerkAssembly"/>
        <tab id="pending" 
	     roles="Owner Signatory"/>
    </state>
    <state id="dropped">
        <tab id="archive" 
	     roles="SpeakerSenate SpeakerAssembly
		    Owner"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" 
	     roles="Owner
		    ClerkSenate ClerkAssembly
		    SpeakerSenate SpeakerAssembly"/>
    </state>
</workspace>
