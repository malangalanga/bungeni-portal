<workspace id="question">
    <state id="working_draft">
        <tab id="draft" roles="Clerk.HeadClerk"/>
    </state>
    <state id="draft">
        <tab id="draft" roles="Owner"/>
    </state>
    <state id="redraft">
        <tab id="draft" roles="Owner"/>
    </state>
    <state id="withdrawn_signatory">
        <tab id="archive" roles="Owner Signatory"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="Clerk.HeadClerk"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="Signatory"/>
        <tab id="pending" roles="Owner Clerk.HeadClerk"/>
    </state>
    <state id="received">
        <tab id="inbox" roles="Clerk.HeadClerk Clerk.QuestionClerk"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="completed">
        <tab id="inbox" roles="Speaker"/>
        <tab id="pending" roles="Clerk.HeadClerk Clerk.QuestionClerk Owner"/>
    </state>
    <state id="admissible">
        <tab id="inbox" roles="Clerk.QuestionClerk"/>
	<tab id="pending" 
	     roles="Clerk.HeadClerk
                    Speaker
		    Signatory
		    Owner"/>
    </state>
    <state id="inadmissible">
        <tab id="archive"
	     roles="Owner 
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker"/>
    </state>
     <state id="clarification_required">
        <tab id="inbox" roles="Owner"/>
	      <tab id="pending" roles="Clerk.HeadClerk  Clerk.QuestionClerk"/>
    </state>
    <state id="incomplete">
        <tab id="archive"
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker"/>
    </state>
    <state id="schedule_pending">
        <tab id="pending" 
	     roles="Owner
		    Signatory
		    Speaker
		    Clerk.HeadClerk
                    Clerk.QuestionClerk"/>
    </state>
    <state id="adjourned">
        <tab id="archive" 
	     roles="Clerk.HeadClerk
                    Clerk.QuestionClerk
		    Speaker
		    Owner 
                    Speaker"/>
    </state>
   <state id="response_pending">
        <tab id="inbox" roles="Minister"/>
	<tab id="pending" 
	     roles="Owner
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
		    Speaker
	            Signatory"/>
    </state>
   <state id="response_submitted">
        <tab id="inbox" roles="Clerk.QuestionClerk"/>
        <tab id="pending" 
	     roles="Clerk.HeadClerk
                    Minister
		    Owner
		    Speaker
	            Signatory"/>
    </state>
   <state id="response_completed">
        <tab id="archive"
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker
		    Minister"/>
    </state>
    <state id="deferred">
        <tab id="archive" 
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker"/>
    </state>
    <state id="debated">
        <tab  id="archive"
	      roles="Owner
		     Signatory
		     Clerk.HeadClerk 
                     Clerk.QuestionClerk
                     Speaker"/>
    </state>
    <state id="elapsed">
        <tab id="archive" 
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker
		    Minister"/>
    </state>
    <state id="dropped">
        <tab id="archive" 
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" 
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk 
                    Speaker"/>
    </state>
    <state id="withdrawn_internal">
        <tab id="archive"
	     roles="Owner
		    Signatory
		    Clerk.HeadClerk
                    Clerk.QuestionClerk
                    Speaker"/>
    </state>
</workspace>
