<workspace>

    <state id="draft">
        <tab id="draft" roles="Drafter" />
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate" />
        <tab id="pending" roles="Owner" />
    </state>
    <state id="not_received">
        <tab id="archive" roles="ClerkSenate Owner" />
    </state>
    <state id="received">
        <tab id="inbox" roles="ClerkSenate" />
        <tab id="pending" roles="SpeakerSenate Owner" />
    </state>
    <state id="information_required">
        <tab id="inbox" roles="Owner" />
        <tab id="pending" roles="SpeakerSenate ClerkSenate" />
    </state>
    <state id="laid_on_the_table">
        <tab id="inbox" roles="ClerkSenate" />
        <tab id="pending" roles="SpeakerSenate Owner" />
    </state>
    <state id="referred">
        <tab id="inbox" roles="MemberCommitteeSenate.Clerk" />
        <tab id="pending" roles="Owner ClerkSenate SpeakerSenate" />
    </state>
    <state id="ready_to_report">
        <tab id="schedule" roles="ClerkSenate" />
        <tab id="pending" roles="Owner SpeakerSenate MemberCommitteeSenate.Clerk" />
    </state>
    <state id="scheduled">
        <tab id="schedule" roles="ClerkSenate" />
        <tab id="pending" roles="Owner SpeakerSenate MemberCommitteeSenate.Clerk" />
    </state>
    <state id="reported">
        <tab id="archive" roles="Owner ClerkSenate SpeakerSenate MemberCommitteeSenate.Clerk" />
    </state>

</workspace>

