<workspace id="motion">
    <state id="working_draft">
        <tab id="draft" roles="bungeni.Clerk"/>
    </state>
    <state id="draft">
        <tab id="draft" roles="bungeni.Owner"/>
    </state>
    <state id="withdrawn_external">
        <tab id="archive" roles="bungeni.Owner bungeni.Clerk
                                 bungeni.Signatory"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner bungeni.Signatory"/>
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="bungeni.Signatory"/>
        <tab id="sent" roles="bungeni.Owner"/>
    </state>
    <state id="received">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner bungeni.Signatory"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="bungeni.Owner"/>
        <tab id="sent" roles="bungeni.Clerk bungeni.Signatory"/>
    </state>
    <state id="completed">
        <tab id="inbox" roles="bungeni.Speaker"/>
        <tab id="sent" roles="bungeni.Owner bungeni.Clerk bungeni.Signatory"/>
    </state>
    <state id="incomplete">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner bungeni.Signatory"/>
    </state>
    <state id="withdrawn_internal">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
     <state id="admissible">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner bungeni.Signatory bungeni.Speaker"/>
    </state>
    <state id="schedule_pending">
        <tab id="sent" roles="bungeni.Owner bungeni.Signatory bungeni.Speaker"/>
    </state>
    <state id="adopted">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="adopted_amendments">
         <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="adjourned">
         <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="deferred">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="elapsed">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="dropped">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="rejected">
         <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" roles="bungeni.Signatory bungeni.Owner bungeni.Clerk
                                 bungeni.Speaker"/>
    </state>
</workspace>
