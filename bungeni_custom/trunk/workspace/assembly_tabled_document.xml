<workspace id="tabled_document">
    <state id="draft">
        <tab id="draft" roles="Drafter" />
    </state>
    <state id="submitted">
        <tab id="inbox" roles="  ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="reviewed">
        <tab id="inbox" roles="  SpeakerAssembly"/>
        <tab id="pending"
	     roles="Owner
		      ClerkAssembly"/>
    </state>
    <state id="admissible">
        <tab id="schedule"
	     roles="  SpeakerAssembly
		      ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="Owner"/>
        <tab id="pending" roles="  ClerkAssembly"/>
    </state>
    <state id="scheduled">
        <tab id="schedule"
	     roles="  SpeakerAssembly
		      ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="adjourned">
        <tab id="schedule"
	     roles="  SpeakerAssembly
		      ClerkAssembly"/>
        <tab id="pending" roles="Owner"/>
    </state>
    <state id="tabled">
        <tab id="archive" 
	     roles="Owner
		      ClerkAssembly
                      SpeakerAssembly"/>
    </state>
    <state id="withdrawn">
        <tab id="archive"
	     roles="Owner
		      ClerkAssembly
                      SpeakerAssembly"/>
    </state>
</workspace>
