<workspace>
    <state id="draft">
        <draft roles="bungeni.Owner"/>
    </state>
    <state id="working_draft">
        <draft roles="bungeni.Clerk"/>
    </state>
    <state id="withdrawn_internal">
        <archive roles="bungeni.Owner"/>
    </state>
    <state id="withdrawn_external">
        <archive roles="bungeni.Owner bungeni.Clerk bungeni.Speaker"/>
    </state>
    <state id="submitted">
        <inbox roles="bungeni.Clerk"/>
        <sent roles="bungeni.Owner"/>
    </state>
    <state id="submitted_signatories">
        <inbox roles="bungeni.Clerk bungeni.Speaker bungeni.Signatory"/>
    </state>
    <state id="received">
        <inbox roles="bungeni.Clerk"/>
        <sent roles="bungeni.Owner"/>
    </state>
    <state id="clarification_required">
        <inbox roles="bungeni.Owner"/>
    </state>
    <state id="completed">
        <inbox roles="bungeni.Speaker"/>
    </state>
    <state id="incomplete">
        <inbox roles="bungeni.Speaker"/>
    </state>
     <state id="inadmissible">
        <inbox roles="bungeni.Speaker"/>
    </state>
    <state id="admissible">
        <inbox roles="bungeni.Speaker"/>
    </state>
    <state id="schedule_pending">
        <inbox roles="bungeni.Speaker"/>
    </state>
    <state id="adjourned">
        <archive roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="deferred">
        <archive roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="elapsed">
        <archive roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="dropped">
        <archive roles="bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="withdrawn">
        <archive roles="bungeni.Owner bungeni.Clerk bungeni.Speaker"/>
    </state>
</workspace>
