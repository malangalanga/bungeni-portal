      factory=".nextprevious.ATFolderNextPrevious" />  

  <browser:page
     name="presentation_view"     
     for="Products.ATContentTypes.interface.IATDocument"
     class=".presentation.PresentationView"
     permission="zope2.View"
      />  

</configure>

