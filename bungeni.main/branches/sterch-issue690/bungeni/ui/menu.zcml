    <!-- Consistent appearance order of menu items:
    
    "context_actions" menu :
         0- 9 : view=0
        10-19 : edit=10, respond=11
        20-29 : workflow=20, versions=21, auditlog=22
        30-39 : files=30
        40-49 : calendar=40, schedule=41, pdf=42, odt=43
        90-99 : delete=99
    
    
    { committee, bill, motion, question, schedule, sitting, agendaitem, 
      party, government, ministry, member, office }_additems menus:
    
        file=20, event=21, signatory=22
        title=30
        sitting=40, agendaitem=41, attendance=42
        (assign-)bill=50
        partymember=60, groupmember=61, committeemember=62, officemember=63
        ministry=70
        address=80
    -->
    
    
