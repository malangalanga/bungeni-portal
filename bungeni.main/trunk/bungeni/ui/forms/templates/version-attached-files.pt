<div i18n:domain="bungeni.ui" tal:omit-tag="">
    <dt id="fieldsetlegend-attached-files" i18n:translate="">
      attached files
    </dt>

    <dd id="fieldset-attached-files">
      <table class="listing">
        <thead>
          <tr>
            <th i18n:translate=""> Title </th>
            <th i18n:translate=""> Type </th>
            <th i18n:translate=""> Status date </th>
          </tr>
        </thead>
        <tbody>
          <tr tal:repeat="data view/results"> 
            <td>
                <a tal:content="data/title" tal:attributes="href data/url">
                </a>
            </td>
            <td tal:content="data/type"> attachment type </td>
            <td tal:content="data/status_date"> status </td>
          </tr>
        </tbody>
      </table>

    </dd>

</div>
