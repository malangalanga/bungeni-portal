     manager="ploned.ui.interfaces.IJavaScriptManager"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="25"
     />
     <browser:viewlet
     name="reports-js"
     for="*"
     class=".js.Reports"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="30"
     />
     <!--Viewlet manager and viewlets to add rss links in page header-->
     <browser:viewletManager
      name="feed-manager"
      provides=".interfaces.IFeedViewletManager"
      permission="zope.Public"
      class="zope.viewlet.manager.WeightOrderedViewletManager"
      />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IBillContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.ICommitteeContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IQuestionContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IMotionContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.ITabledDocumentContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IAgendaItemContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IGroupSittingContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IReportContainer"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IQuestion"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IMotion"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.ITabledDocument"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <browser:viewlet
     name="rss-viewlet"
     for="bungeni.models.interfaces.IAgendaItem"
     class=".viewlets.feedresource.RSSFeedViewlet"
     manager=".interfaces.IFeedViewletManager"
     permission="zope.View"
     weight="5"
     layer=".interfaces.IRSSRepresentationLayer"
     />
     <!-- Loads the appropriate Dhtmlxscheduler language translation -->
     <browser:viewlet
     name="dhtmlxscheduler-main-language-js"
     for="bungeni.core.interfaces.ISchedulingContext"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     class=".js.dhtmlxscheduler.DhtmlxSchedulerMainLanguage"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="50"
     />
     <browser:viewlet
     name="dhtmlscheduler-recurring-language-js"
     for="bungeni.core.interfaces.ISchedulingContext"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     class=".js.dhtmlxscheduler.DhtmlxSchedulerRecurringLanguage"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="55"
     />
     
	 <!-- Hihglight resources -->
	 <resourceLibrary name="highlight">
	 	 <directory
			 source="js/highlight"
			 include="jquery-highlight-min.js highlight.css"
		 />
	 </resourceLibrary>
</configure>
