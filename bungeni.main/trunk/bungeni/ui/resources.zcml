  <browser:viewlet
     name="jquery-draggable"
     for="*"
     class=".js.Draggable"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="20"
     />

  <browser:viewlet
     name="jquery-resizable"
     for="*"
     class=".js.Resizable"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="25"
     />

  <browser:viewlet
     name="jquery-droppable"
     for="*"
     class=".js.Droppable"
     manager="ploned.ui.interfaces.IJavaScriptManager"
     permission="zope.Public"
     layer="ploned.ui.interfaces.IPlonedSkin"
     weight="25"
     />
  <browser:viewlet
     name="jquery-datepicker"
     for="*"
     class=".js.Datepicker"
