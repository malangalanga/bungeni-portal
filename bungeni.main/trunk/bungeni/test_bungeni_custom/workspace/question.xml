<workspace id="question">
    <state id="working_draft">
        <tab id="draft" roles="bungeni.Clerk.HeadClerk"/>
    </state>
    <state id="draft">
        <tab id="draft" roles="bungeni.Owner"/>
    </state>
    <state id="redraft">
        <tab id="draft" roles="bungeni.Owner"/>
    </state>
    <state id="withdrawn_signatory">
        <tab id="archive" roles="bungeni.Owner bungeni.Signatory"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="bungeni.Clerk.HeadClerk"/>
        <tab id="pending" roles="bungeni.Owner"/>
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="bungeni.Signatory"/>
        <tab id="pending" roles="bungeni.Owner bungeni.Clerk.HeadClerk"/>
    </state>
    <state id="received">
        <tab id="inbox" roles="bungeni.Clerk.HeadClerk bungeni.Clerk.QuestionClerk"/>
        <tab id="pending" roles="bungeni.Owner"/>
    </state>
    <state id="completed">
        <tab id="inbox" roles="bungeni.Speaker"/>
        <tab id="pending" roles="bungeni.Clerk.HeadClerk bungeni.Clerk.QuestionClerk bungeni.Owner"/>
    </state>
    <state id="admissible">
        <tab id="inbox" roles="bungeni.Clerk.QuestionClerk"/>
	<tab id="pending" 
	     roles="bungeni.Clerk.HeadClerk
                    bungeni.Speaker
		    bungeni.Signatory
		    bungeni.Owner"/>
    </state>
    <state id="inadmissible">
        <tab id="archive"
	     roles="bungeni.Owner 
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker"/>
    </state>
     <state id="clarification_required">
        <tab id="inbox" roles="bungeni.Owner"/>
	      <tab id="pending" roles="bungeni.Clerk.HeadClerk  bungeni.Clerk.QuestionClerk"/>
    </state>
    <state id="incomplete">
        <tab id="archive"
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker"/>
    </state>
    <state id="schedule_pending">
        <tab id="pending" 
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Speaker
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk"/>
    </state>
    <state id="adjourned">
        <tab id="archive" 
	     roles="bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
		    bungeni.Speaker
		    bungeni.Owner 
                    bungeni.Speaker"/>
    </state>
   <state id="response_pending">
        <tab id="inbox" roles="bungeni.Minister"/>
	<tab id="pending" 
	     roles="bungeni.Owner
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
		    bungeni.Speaker
	            bungeni.Signatory"/>
    </state>
   <state id="response_submitted">
        <tab id="inbox" roles="bungeni.Clerk.QuestionClerk"/>
        <tab id="pending" 
	     roles="bungeni.Clerk.HeadClerk
                    bungeni.Minister
		    bungeni.Owner
		    bungeni.Speaker
	            bungeni.Signatory"/>
    </state>
   <state id="response_completed">
        <tab id="archive"
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker
		    bungeni.Minister"/>
    </state>
    <state id="deferred">
        <tab id="archive" 
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker"/>
    </state>
    <state id="debated">
        <tab  id="archive"
	      roles="bungeni.Owner
		     bungeni.Signatory
		     bungeni.Clerk.HeadClerk 
                     bungeni.Clerk.QuestionClerk
                     bungeni.Speaker"/>
    </state>
    <state id="elapsed">
        <tab id="archive" 
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker
		    bungeni.Minister"/>
    </state>
    <state id="dropped">
        <tab id="archive" 
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" 
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk 
                    bungeni.Speaker"/>
    </state>
    <state id="withdrawn_internal">
        <tab id="archive"
	     roles="bungeni.Owner
		    bungeni.Signatory
		    bungeni.Clerk.HeadClerk
                    bungeni.Clerk.QuestionClerk
                    bungeni.Speaker"/>
    </state>
</workspace>
